{
  "name": "mosaico-disqus",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^14.16.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "disqus-react": "^1.1.2",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' STAGE=${STAGE:-dev} FORCE_CLIENT_SIDE_FETCH=${FORCE_CLIENT_SIDE_FETCH:-false} BRAND=${BRAND:-mosaico} node server.js",
    "build": "STAGE=${STAGE:-dev} BUILD_TIME=true BRAND=${BRAND:-mosaico npm run build",
    "start": "STAGE=${STAGE:-dev} BRAND=${BRAND:-mosaico} NODE_ENV=dev -i ${VCPU:-1} node server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
